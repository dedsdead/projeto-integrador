<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.Group?>

<VBox prefHeight="800.0" prefWidth="800.0" spacing="10.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ifpr.pgua.eic.trabalhofinal.controllers.TelaFotos">
    <Group fx:id="gpImagens" />
    <TextField fx:id="tfFoto" />
    <HBox>
        <Button fx:id="btBuscarFoto" onAction="#buscarFoto" text="Selecionar foto" />
        <Button fx:id="btSalvarFoto" onAction="#salvarFoto" text="Salvar foto" />
        <Button fx:id="btSair" onAction="#sair" text="Sair" />
    </HBox>
</VBox>