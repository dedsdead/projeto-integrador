<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.Pane?>

<VBox prefHeight="800.0" prefWidth="800.0" spacing="10.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ifpr.pgua.eic.trabalhofinal.controllers.TelaFotos">
    <children>
        <Pane prefHeight="46.0" prefWidth="780.0">
            <children>
                <Label alignment="CENTER" layoutX="1.0" layoutY="4.0" prefHeight="16.0" prefWidth="780.0" text="Fotos" />
            </children>
        </Pane>
        <TilePane fx:id="painel"  prefHeight="450.0" prefWidth="800.0"/>
        <TextField fx:id="tfFoto" />
        <HBox>
            <Button fx:id="btSelecionarFoto" onAction="#selecionarFoto" text="Selecionar foto" />
            <Button fx:id="btSalvarFoto" onAction="#salvarFoto" text="Salvar foto" />
            <Button fx:id="btSair" onAction="#sair" text="Sair" />
        </HBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>